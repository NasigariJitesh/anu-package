{
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "anu/*": ["../../packages/anu/*"],
      "lib/*": ["../../packages/anu/lib/*"],
      "common/*": ["../../packages/anu/common/*"],
      "config/*": ["../../packages/anu/config/*"],
      "src/*": ["src/*"],
      "screens": ["src/screens"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["globals.d.ts", "next-env.d.ts", "rn-overrides.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
