{
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "jsx": "preserve",
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "app-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
