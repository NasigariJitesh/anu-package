{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "test:anu": "cd packages/anu && yarn run test",
    "dev": "turbo run dev",
    "dev:docs": "cd apps/docs && yarn run dev",
    "lint": "turbo run lint -- --cache --cache-strategy content --fix",
    "format": "prettier --write . ",
    "clean": "npx rimraf packages/**/node_modules apps/**node_modules node_modules **/.eslintcache",
    "clean-install": "yarn clean && yarn",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "husky": "latest",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.4.1"
}
