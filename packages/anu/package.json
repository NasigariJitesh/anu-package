{
  "name": "anu",
  "version": "1.0.0",
  "homepage": "https://github.com/NasigariJitesh/anu-package#readme",
  "repository": "https://github.com/NasigariJitesh/anu-package",
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "release": "release-it",
    "pods": "node -e \"process.exit(require('os').platform() !== 'darwin')\" && test -d example/ios && cd example/ios && pod install || printf ''",
    "bootstrap": "yarn example && yarn && yarn pods",
    "build": "npx expo-module build",
    "clean": "yarn expo-module clean",
    "test": "yarn expo-module test",
    "prepare": "npx expo-module prepare"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^11.5.4",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.194",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "jest": "^29.5.0",
    "jest-expo": "^48.0.2",
    "react": "^18.2.0",
    "react-native-builder-bob": "^0.20.4",
    "tsconfig": "*",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "@material/material-color-utilities": "^0.2.5",
    "@miblanchard/react-native-slider": "^2.3.1",
    "compressorjs": "^1.2.1",
    "dripsy": "^3.6.0",
    "expo-blur": "^12.2.2",
    "expo-document-picker": "11.2.2",
    "expo-haptics": "~12.0.1",
    "expo-image-manipulator": "^11.1.1",
    "expo-linear-gradient": "^12.1.2",
    "libphonenumber-js": "^1.10.28",
    "lodash": "^4.17.21",
    "moti": "^0.25.3",
    "react-dropzone": "^14.2.3"
  },
  "peerDependencies": {
    "expo": ">=47",
    "next": "*",
    "react": ">=18",
    "react-native-gesture-handler": "*",
    "react-native-reanimated": ">=3",
    "react-native-safe-area-context": "*",
    "react-native-vector-icons": "*"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "index.d.ts",
  "react-native": "./index.ts",
  "source": "./index.ts",
  "files": [
    "!**/*.test.ts",
    "!**/*.test.tsx",
    "!**/__fixtures__",
    "!**/__mocks__",
    "jest.config.js"
  ],
  "@react-native-community/bob": {
    "source": ".",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
