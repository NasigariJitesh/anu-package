{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"esModuleInterop": true,
		"jsx": "react-native",
		"lib": ["DOM", "ESNext"],
		"moduleResolution": "node",
		"noEmit": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"target": "ESNext",

		"baseUrl": ".",
		"allowJs": false,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": true,
		"module": "esnext",
		"isolatedModules": true
	}
}
